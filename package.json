{
  "name": "@mcpepc/site",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "test": "xo",
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "type": "module",
  "dependencies": {
    "@sveltejs/adapter-cloudflare": "^1.0.0-next.2",
    "knockout": "^3.5.1",
    "survey-knockout": "^1.9.0"
  },
  "devDependencies": {
    "@sveltejs/kit": "next",
    "@types/cookie": "^0.4.1",
    "autoprefixer": "^10.4.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "postcss": "^8.4.5",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "svelte2tsx": "^0.4.10",
    "tailwindcss": "^3.0.7",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "xo": "^0.47.0"
  },
  "xo": {
    "plugins": [
      "svelte3"
    ],
    "overrides": [
      {
        "files": [
          "*.svelte"
        ],
        "processor": "svelte3/svelte3"
      }
    ],
    "settings": {
      "svelte3/typescript": true
    }
  }
}
